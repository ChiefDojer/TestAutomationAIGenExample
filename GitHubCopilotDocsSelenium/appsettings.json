{
  "BaseUrl": "https://docs.github.com",
  "Browser": {
    "Type": "chrome",
    "Headless": true,
    "ViewportWidth": 1920,
    "ViewportHeight": 1080,
    "ScreenshotOnFailure": true,
    "Maximize": false,
    "AcceptInsecureCerts": false
  },
  "Execution": {
    "DefaultTimeout": 30000,
    "PageLoadTimeout": 60000,
    "MaxRetries": 3,
    "RetryDelay": 1000,
    "Parallel": true,
    "Workers": 0
  },
  "Logging": {
    "MinimumLevel": "Information",
    "OutputPath": "TestResults/Logs",
    "Console": true,
    "File": true
  }
}
